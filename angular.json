{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "SmartCrop-App5": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "es-ES": {
            "translation": "src/locale/messages.es-ES.xlf",
            "baseHref": "/es-ES/"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular/build:application",


          "options": {




            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [

                "src/favicon.ico",
                "src/assets"

            ],
            "styles": [
              "src/styles.css"
            ],
            "polyfills": [
              "@angular/localize/init"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "en-US": {
              "localize": ["en-US"],
              "outputPath": "dist/SmartCrop-App5"
            },
            "es-ES": {
              "localize": ["es-ES"],
              "outputPath": "dist/SmartCrop-App5/es-ES"
            }
          },
          "defaultConfiguration": "en-US"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "SmartCrop-App5:build:production"
            },
            "development": {
              "buildTarget": "SmartCrop-App5:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n",
          "options": {
            "buildTarget": "SmartCrop-App5:build"
          }
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "polyfills": [
              "@angular/localize/init"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "7255ee32-b967-400e-9de2-bf4d609ecf74"
  }
}
